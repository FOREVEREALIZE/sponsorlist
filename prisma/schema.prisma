generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Brand {
  id           String   @id @default(cuid())
  name         String
  aliases      String[]
  companies    String[]
  overallScore Int
  description  String?
  website      String?
  notes        String?
}
